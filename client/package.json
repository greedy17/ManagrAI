{
  "name": "managr",
  "version": "1.0.0",
  "private": true,
  "description": "Managr Application",
  "scripts": {
    "dev": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:e2e",
    "ci": "vue-cli-service test:e2e --headless",
    "heroku-postbuild": "npm run build",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.16.0",
    "@datadog/browser-rum": "^2.6.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^2.0.10",
    "@stripe/stripe-js": "^2.2.0",
    "apexcharts": "^3.37.1",
    "axios": "^1.6.0",
    "chart.js": "^2.9.4",
    "crypto-js": "^4.1.1",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^3.1.2",
    "lodash.debounce": "^4.0.8",
    "lodash.isplainobject": "^4.0.6",
    "lodash.throttle": "^4.1.1",
    "luxon": "^1.24.1",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "v-mask": "^2.2.1",
    "vue": "^2.5.22",
    "vue-apexcharts": "^1.6.2",
    "vue-chartjs": "^3.5.1",
    "vue-clipboard2": "^0.3.3",
    "vue-datetime": "^1.0.0-beta.13",
    "vue-drag-drop": "^1.1.4",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-multiselect": "^2.1.6",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.2",
    "vue-sanitize": "^0.2.1",
    "vue-scrollto": "^2.17.1",
    "vue-template-compiler": "^2.6.10",
    "vue-toastification": "^1.7.14",
    "vue2-google-maps": "^0.10.7",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.5.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-e2e-cypress": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^4.6.0",
    "cypress": "^3.1.5",
    "eslint": "^5.15.1",
    "eslint-plugin-vue": "^5.2.2",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "contributors": [
    {
      "name": "William Huster",
      "email": "william@thinknimble.com"
    },
    {
      "name": "Neil Shah",
      "email": "neil@thinknimble.com"
    }
  ]
}
