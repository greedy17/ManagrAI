{
  "name": "managr",
  "version": "1.0.0",
  "private": true,
  "description": "Managr Application",
  "scripts": {
    "dev": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:e2e",
    "ci": "vue-cli-service test:e2e --headless",
    "heroku-postbuild": "npm run build",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "@thinknimble/tn-utils": "^1.4.0",
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isplainobject": "^4.0.6",
    "lodash.throttle": "^4.1.1",
    "luxon": "^1.24.1",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "pluralize": "^8.0.0",
    "randomcolor": "^0.5.3",
    "v-mask": "^2.2.1",
    "vue": "^2.5.22",
    "vue-datetime": "^1.0.0-beta.13",
    "vue-router": "^3.0.2",
    "vue-scrollto": "^2.17.1",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.5.4",
    "weekstart": "^1.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-e2e-cypress": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^4.6.0",
    "cypress": "^3.1.5",
    "eslint": "^5.15.1",
    "eslint-plugin-vue": "^5.2.2",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "contributors": [
    {
      "name": "William Huster",
      "email": "william@thinknimble.com"
    },
    {
      "name": "Neil Shah",
      "email": "neil@thinknimble.com"
    }
  ]
}
